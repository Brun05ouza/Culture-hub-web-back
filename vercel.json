{
  "version": 2,
  "builds": [
    {
      "src": "api/server-new.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/server-new.js"
    },
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|webp|ico|html))",
      "dest": "/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/telaPrincipal.html"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server-new.js"
    }
  ]
}
